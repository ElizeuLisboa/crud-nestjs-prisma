datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Cliente {
  id        Int      @id @default(autoincrement())
  nome      String
  email     String
  cpf       String   @unique
  criadoEm  DateTime @default(now())
  updatedAt DateTime @updatedAt
}

