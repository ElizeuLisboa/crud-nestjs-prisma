{
  "name": "nest-crud-clientes",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "nest start",
    "start:prod": "node dist/main.js",
    "postinstall": "prisma generate",
    "start:debug": "cross-env PORT=4001 nest start --debug --watch",
    "start:dev": "nest start --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "cross-env NODE_ENV=test DATABASE_URL='file:./prisma/dev-test.db' jest --config jest.integration.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "migrate:dev": "prisma migrate dev --name init_dev",
    "migrate:test": "cross-env DATABASE_URL=file:./prisma/dev-test.db prisma migrate dev --name init_test",
    "seed": "ts-node prisma/seed.ts",
    "seed-cruddb": "ts-node prisma/seed-cruddb.ts",
    "seed-postgres": "ts-node prisma/seed-postgres.ts",
    "prisma:seed": "prisma db seed",
    "seed:test": "cross-env DATABASE_URL=file:./prisma/dev-test.db ts-node prisma/seed-test.ts"
  },
  "dependencies": {
    "@jest/types": "^30.0.5",
    "@nestjs/common": "10.4.22",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.22",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.22",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "6.19.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.0",
    "mercadopago": "^2.11.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^2.2.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.4",
    "react-qr-code": "^2.0.18",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "stripe": "^12.18.0",
    "sucrase": "^3.35.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "10.4.22",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/paypal__checkout-server-sdk": "^1.0.8",
    "@types/qrcode": "^1.5.6",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "prisma": "6.19.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "resolutions": {
    "@types/passport": "^1.0.12"
  }
}
