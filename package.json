{
  "name": "nest-crud-clientes",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:debug": "cross-env PORT=4001 nest start --debug --watch",  
    "start:dev": "nest start --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "cross-env NODE_ENV=test DATABASE_URL='file:./prisma/dev-test.db' jest --config jest.integration.config.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "migrate:dev": "prisma migrate dev --name init_dev",
    "migrate:test": "cross-env DATABASE_URL=file:./prisma/dev-test.db prisma migrate dev --name init_test",
    "seed": "ts-node prisma/seed.ts",
    "seed:test": "cross-env DATABASE_URL=file:./prisma/dev-test.db ts-node prisma/seed-test.ts"
  },
  "dependencies": {
    "@jest/types": "^30.0.5",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.0",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "stripe": "^12.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/passport": "^0",
    "@types/passport-jwt": "^4.0.1",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "prisma": "^6.12.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "resolutions": {
    "@types/passport": "^1.0.12"
  },
  "packageManager": "yarn@4.1.1+sha512.ec40d0639bb307441b945d9467139cbb88d14394baac760b52eca038b330d16542d66fef61574271534ace5a200518dabf3b53a85f1f9e4bfa37141b538a9590"
}
